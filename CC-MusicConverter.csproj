<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>CC_MusicConverter</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="res\LionRay.jar" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="res\LionRay.jar" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="res\LionRay.jar" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ChaseLabs.Logger" Version="0.2.1.1" />
    <PackageReference Include="Xabe.FFmpeg.Downloader" Version="5.1.0" />
  </ItemGroup>

</Project>
